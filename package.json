{
  "name": "solid-error",
  "version": "0.2.2-alpha",
  "description": "Print meaningful errors to the terminal.",
  "main": "dist/index.js",
  "scripts": {
    "precommit": "npm test",
    "build": "rm -rf ./dist/*.* && babel ./src --out-dir ./dist",
    "test": "npm run build && ./node_modules/.bin/jest --coverage && codecov",
    "plato": "./node_modules/.bin/plato -e .eslintrc.json -r -d report ./src",
    "docs": "./node_modules/.bin/jsdoc -c .jsdocrc"
  },
  "author": "Roberto Mauro <erremauro@icloud.com>",
  "keyword": "error, stack, err, console, pretty, prettyprint",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.13.2",
    "codecov": "^1.0.1",
    "eslint": "~3.2.0",
    "eslint-config-node": "^1.4.0",
    "husky": "^0.11.6",
    "jest-cli": "^14.1.0",
    "jsdoc": "^3.4.0",
    "plato": "^1.7.0"
  },
  "dependencies": {
    "cardinal": "^0.7.1",
    "chalk": "^1.1.3",
    "cli-table": "^0.3.1",
    "js-yaml": "^3.6.1",
    "marked": "^0.3.6",
    "node-emoji": "^1.3.1"
  }
}
